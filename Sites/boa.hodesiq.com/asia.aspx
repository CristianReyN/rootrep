        function CheckKey(evt, div, img) 
        {
            evt = (evt) ? evt : ((window.event) ? window.event : "")
            if (evt) {
                // process event here
                if (evt.keyCode == 13 || evt.which == 13) {
                    SwapDiv(div, img)
                }
            }
